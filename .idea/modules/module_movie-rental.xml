<?xml version="1.0" encoding="UTF-8"?>
<project name="module_movie-rental" default="compile.module.movie-rental">
  <dirname property="module.movie-rental.basedir" file="${ant.file.module_movie-rental}"/>
  
  <property name="module.jdk.home.movie-rental" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.movie-rental" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.movie-rental" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.movie-rental" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="movie-rental.output.dir" value="${module.movie-rental.basedir}/../../classes/production/movie-rental"/>
  <property name="movie-rental.testoutput.dir" value="${module.movie-rental.basedir}/../../classes/test/movie-rental"/>
  
  <path id="movie-rental.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="movie-rental.module.production.classpath">
    <path refid="${module.jdk.classpath.movie-rental}"/>
    <pathelement location="${basedir}/../../../Downloads/postgresql-42.0.0.jar"/>
    <path refid="library.postgresql-42.0.0.classpath"/>
  </path>
  
  <path id="movie-rental.runtime.production.module.classpath">
    <pathelement location="${movie-rental.output.dir}"/>
    <pathelement location="${basedir}/../../../Downloads/postgresql-42.0.0.jar"/>
    <path refid="library.postgresql-42.0.0.classpath"/>
  </path>
  
  <path id="movie-rental.module.classpath">
    <path refid="${module.jdk.classpath.movie-rental}"/>
    <pathelement location="${movie-rental.output.dir}"/>
    <pathelement location="${basedir}/../../../Downloads/postgresql-42.0.0.jar"/>
    <path refid="library.postgresql-42.0.0.classpath"/>
  </path>
  
  <path id="movie-rental.runtime.module.classpath">
    <pathelement location="${movie-rental.testoutput.dir}"/>
    <pathelement location="${movie-rental.output.dir}"/>
    <pathelement location="${basedir}/../../../Downloads/postgresql-42.0.0.jar"/>
    <path refid="library.postgresql-42.0.0.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.movie-rental">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.movie-rental">
    <patternset refid="excluded.from.module.movie-rental"/>
  </patternset>
  
  
  <target name="compile.module.movie-rental" depends="compile.module.movie-rental.production,compile.module.movie-rental.tests" description="Compile module movie-rental"/>
  
  <target name="compile.module.movie-rental.production" depends="register.custom.compilers" description="Compile module movie-rental; production classes"/>
  
  <target name="compile.module.movie-rental.tests" depends="register.custom.compilers,compile.module.movie-rental.production" description="compile module movie-rental; test classes" unless="skip.tests"/>
  
  <target name="clean.module.movie-rental" description="cleanup module">
    <delete dir="${movie-rental.output.dir}"/>
    <delete dir="${movie-rental.testoutput.dir}"/>
  </target>
</project>